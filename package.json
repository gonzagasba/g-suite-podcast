{
  "name": "gonzagasba-g-suite-podcast",
  "version": "1.0.0",
  "description": "Interactive podcast browser for Gonzaga University G-Suite Podcast",
  "homepage": "https://gonzagasba.github.io/g-suite-podcast",
  "repository": {
    "type": "git",
    "url": "https://github.com/gonzagasba/g-suite-podcast.git"
  },
  "keywords": [
    "podcast",
    "gonzaga",
    "business",
    "education",
    "arcgis"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1"
  },
  "scripts": {
    "start": "react-scripts start",
